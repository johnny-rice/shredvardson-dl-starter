# Local development environment variables
NEXT_PUBLIC_APP_NAME=DL Starter
NEXT_PUBLIC_ENABLE_ANALYTICS=true

# Supabase (Local Development)
# Copy these from `supabase status` output after running `supabase start`
NEXT_PUBLIC_SUPABASE_URL=http://127.0.0.1:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-local-anon-key-here

# For production deployment, replace with your Supabase cloud project keys:
# NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
# NEXT_PUBLIC_SUPABASE_ANON_KEY=your-cloud-anon-key

# Optional: Application URL for auth callbacks (defaults to http://localhost:3000 if not set)
# NEXT_PUBLIC_APP_URL=http://localhost:3000
