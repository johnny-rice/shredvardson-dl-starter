{
  "name": "dl-starter-new",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "prepare": "lefthook install",
    "dev": "turbo run dev --filter=web",
    "build": "turbo run build",
    "start": "turbo run start --filter=web",
    "lint": "turbo run lint",
    "format": "prettier -w .",
    "pretest": "tsx scripts/pretest-env-check.ts",
    "test": "npm run pretest && turbo run test",
    "test:unit": "npm run pretest && turbo run test:unit",
    "test:e2e": "npm run pretest && turbo run test:e2e --filter=web",
    "test:e2e:ci": "tsx scripts/pretest-env-check.ts && playwright test --config playwright.config.ts",
    "test:coverage": "npm run pretest && turbo run test:coverage --filter=web",
    "test:ci-scripts": "bash tests/ci/validate-specs.test.sh",
    "test:analyze-performance": "node scripts/analyze-test-performance.mjs",
    "a11y:audit": "tsx scripts/accessibility/run-axe.ts",
    "a11y:audit:critical": "tsx scripts/accessibility/run-axe.ts full critical",
    "typecheck": "turbo run typecheck",
    "specs:validate": "bash scripts/ci/validate-specs.sh",
    "ci:validate": "pnpm run specs:validate",
    "debug": "tsx scripts/debug.ts",
    "debug:sentry": "tsx scripts/debug.ts sentry:check",
    "debug:env": "tsx scripts/debug.ts env:check",
    "debug:health": "tsx scripts/debug.ts dev:health",
    "db:types": "pnpm --filter=@shared/db run db:types",
    "db:migrate": "pnpm --filter=@shared/db run db:migrate",
    "db:migrate:create": "tsx scripts/db/migrate.ts create",
    "db:migrate:validate": "tsx scripts/db/migrate.ts validate",
    "db:migrate:apply": "tsx scripts/db/migrate.ts apply",
    "db:migrate:rollback": "tsx scripts/db/migrate.ts rollback",
    "db:reset": "pnpm --filter=@shared/db run db:reset",
    "db:diff": "pnpm --filter=@shared/db run db:diff",
    "db:status": "pnpm --filter=@shared/db run db:status",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:plan": "tsx scripts/db/plan.ts",
    "db:rls:scaffold": "tsx scripts/db/rls-scaffold.ts",
    "db:validate": "tsx scripts/validate-migration.ts",
    "db:validate:ci": "tsx scripts/db/ci-validate.ts",
    "db:validate:rls": "tsx scripts/db/validate-rls.ts",
    "db:seed:dev": "tsx scripts/seed-dev.ts",
    "db:seed:test": "tsx scripts/seed-test.ts",
    "test:rls": "supabase test db",
    "test:rls:watch": "supabase test db --watch",
    "skill:measure": "tsx scripts/skills/measure-tokens.ts",
    "new-app": "tsx scripts/new-app.ts",
    "new-repo": "tsx scripts/new-repo-from-template.ts",
    "doctor": "tsx scripts/starter-doctor.ts",
    "doctor:report": "tsx scripts/starter-doctor.ts --report artifacts/doctor-report.md",
    "audit:large": "node scripts/audit-large-files.mjs",
    "audit:traceability": "tsx scripts/validate-traceability.ts",
    "adr:create": "tsx scripts/new-adr.ts",
    "ai-review": "tsx scripts/ai-review-analyzer.ts",
    "ai-review:report": "tsx scripts/ai-review-analyzer.ts --report",
    "preflight": "tsx scripts/preflight.ts",
    "constitution:update": "tsx scripts/update-constitution-checksum.ts",
    "check:react-versions": "node scripts/check-react-versions.mjs",
    "preflight:placeholders": "bash -lc 'PATTERN=\"{{\"; PATTERN+=\"PLACEHOLDER\"; PATTERN+=\"}}\"; ! grep -R \"$PATTERN\" . -n --exclude-dir=node_modules --exclude-dir=.turbo'",
    "git:start": "bash scripts/git-workflow.sh start",
    "git:status": "bash scripts/git-workflow.sh status",
    "git:cleanup": "bash scripts/git-workflow.sh cleanup-dry",
    "git:cleanup-force": "bash scripts/git-workflow.sh cleanup",
    "git:finish": "bash scripts/git-workflow.sh finish-branch",
    "hooks:install": "bash scripts/git-hooks/install.sh",
    "docs:check": "tsx scripts/docs-check.ts",
    "pr:create": "tsx scripts/create-pr.ts",
    "learn:index": "node scripts/generate-learnings-index.js",
    "learn:update-reuse": "bash scripts/update-micro-lesson-reuse.sh",
    "learn:archive": "node scripts/archive-micro-lessons.js --older-than 90 --unused",
    "commands:generate": "node scripts/generate-command-index.js",
    "commands:check": "node scripts/generate-command-index.js && git diff --exit-code docs/commands/index.json || (echo 'Command index out of sync. Run: npm run commands:generate' && exit 1)",
    "commands:validate-inventory": "node scripts/validate-command-inventory.js",
    "new-spec": "node scripts/new-spec.mjs",
    "telemetry:report": "tsx scripts/telemetry-report.ts",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:tag": "changeset tag",
    "changeset:publish": "changeset publish",
    "release:preflight": "test -z \"$(git status --porcelain)\" || (echo 'Working tree not clean. Commit, stash, or clean untracked files.' && exit 1)",
    "release": "pnpm release:preflight && pnpm changeset:version && git add -A && git diff --cached --quiet || git commit -m 'chore(release): version bumps'; pnpm changeset:tag && git push --follow-tags",
    "clean": "rm -rf .turbo coverage test-results playwright-report 2>/dev/null || true; find . -type f -name '*.tsbuildinfo' -print -delete 2>/dev/null || true",
    "clean:cache": "rm -rf \"$(git rev-parse --show-toplevel)/apps/web/.next\" \"$(git rev-parse --show-toplevel)/.turbo\" \"$(git rev-parse --show-toplevel)/node_modules/.cache\" && echo '✅ Build caches cleared (Next.js, Turbo, pnpm)'",
    "clean:install": "pnpm -w clean:cache && pnpm install && echo '✅ Fresh install complete'"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@biomejs/biome": "^2.3.0",
    "@changesets/cli": "^2.29.6",
    "@eslint/eslintrc": "^3.1.0",
    "@faker-js/faker": "^10.1.0",
    "@playwright/test": "^1.56.1",
    "@types/handlebars": "^4.1.0",
    "commander": "^14.0.1",
    "eslint": "^9.38.0",
    "fast-glob": "^3.3.3",
    "glob": "^11.0.3",
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.8",
    "lefthook": "^2.0.1",
    "markdownlint-cli2": "^0.18.1",
    "prettier": "^3.6.2",
    "tiktoken": "^1.0.22",
    "tsx": "^4.20.6",
    "turbo": "^2.5.8",
    "typescript": "^5",
    "yaml": "^2.8.1"
  },
  "overrides": {
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-is": "19.1.1",
    "zod": "3.23.8"
  },
  "packageManager": "pnpm@9.12.0",
  "dependencies": {
    "lucide-react": "^0.553.0"
  }
}
